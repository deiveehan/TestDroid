pipeline {
    agent any

    tools {
        gradle '7.5.1'
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'gradle --version'
                echo 'Gradle: $GRADLE_HOME'
                sh 'gradle clean assembleDebug'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
    //                 git 'https://github.com/deiveehan/TestDroid.git'

}